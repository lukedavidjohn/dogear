{
  "name": "DogEar",
  "version": "1.0",
  "description": "Dynamic bookmark searching.",
  "permissions": ["bookmarks", "https://s2.googleusercontent.com/*"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "dogear-black-128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Space",
        "mac": "Command+Shift+Space"
      },
      "description": "opens pop-up"
    }
  },
  "icons": {
    "128": "./dogear-white-128.png"
  },
  "manifest_version": 2
}
